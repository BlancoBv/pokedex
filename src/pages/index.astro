---
import Layout from '../layouts/Layout.astro';
import PokemonVisor from '../components/PokemonVisor.vue';

interface pokemon {name:string; url: string; }
interface response {count: number; next: string; previus:string | null; results: pokemon[] }

const pokemons: response = await fetch("https://pokeapi.co/api/v2/pokemon").then(res => res.json()).catch(() => null)
---

<Layout title="Welcome to Astro.">
	<PokemonVisor client:load items={pokemons.results} nextUrl={pokemons.next}>
</Layout>



